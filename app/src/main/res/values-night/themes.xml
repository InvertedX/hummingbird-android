<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.HummingBird" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryVariant">@color/colorPrimaryVariant</item>
        <item name="colorOnPrimary">@color/black</item>
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/gray_600</item>
        <item name="android:windowBackground">#212121</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="toolbarStyle">@style/Theme.ToolBar</item>
    </style>

    <style name="Theme.ToolBar" parent="Widget.MaterialComponents.Toolbar">
        <item name="backgroundColor">?attr/colorPrimaryVariant</item>
        <item name="background">?attr/colorPrimaryVariant</item>
    </style>


</resources>